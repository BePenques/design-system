{
    "compilerOptions": {
      "composite": false,
      "declaration": true,
      "declarationMap": true,
      "esModuleInterop": true,        
      "forceConsistantCasingInFileNmaes": true,
      "inLineSources": false,
      "isolateModules": true,
      "moduleResolution": "Node",      
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "preserveWatchOutput": true,
     "skipLibCheck": true,             
     "strict": true                  
    },
    "exclude": ["node_modules"]
  }
  

